    "@langchain/community": "^0.3.20",
    "@langchain/mistralai": "^0.2.0",
    "@langchain/mongodb": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "langchain": "^0.3.8",
    "lodash": "^4.17.21",
    "madge": "^8.0.0",
    "mongodb": "^6.12.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/lodash": "^4.17.13",
    "@types/multer": "^1.4.12",
    "@types/swagger-jsdoc": "^6.0.4",
